@{
    ViewBag.Title = "School Information Editor";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2><a href="javascript:void(0);" id="back" onclick="history.go(-1);"></a> &nbsp;School Information Editor</h2>

<style>
    ._panel { background: #fafafa; border: solid 1px #cccccc; padding: 10px; border-radius: 6px; }
</style>
<br />
<form class="form-horizontal" role="form">

 
        <div class="form-group">
            <label class="control-label col-sm-2">School Name</label>
            <div class="col-sm-4">
                <input type="text" class="form-control" id="parent" placeholder="School Name">
            </div>
        </div>
 
    <div class="form-group">
        <div class="col-sm-offset-2 col-sm-4">
            <button type="submit" class="btn btn-default">Save Information</button>
        </div>
    </div>
</form>